{
    "version": 2,
    "public": true,
    "build": { "env": { "DB": "../hexo_circle_of_friends/data.db" } },
    "builds": [{ "src": "main.py", "use": "@vercel/python" }],
    "routes": [
        { "src": "/api", "dest": "main.py" },
        { "src": "/friend", "dest": "main.py" },
        { "src": "/randomfriend", "dest": "main.py" },
        { "src": "/randompost", "dest": "main.py" },
        { "src": "/post", "dest": "main.py" },
        { "src": "/postjson", "dest": "main.py" },
        { "src": "/docs", "dest": "main.py" },
        { "src": "/openapi.json", "dest": "main.py" }

    ]
}